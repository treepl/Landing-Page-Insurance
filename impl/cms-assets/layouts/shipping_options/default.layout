<div class="cms_fake_select">
    <select data-cms_cart_shipping_options="">
        <option value="">Please select</option>
        {% for item in this.items %}
        <option value="{{item.Id}}" {% if  shoppingCartData.shippingOption.shippingOptionId == item.id %} selected="true" {% endif %}>{{item.Name}} ({{item.TotalPrice  | domain_money_format }})</option>
        {% endfor %}
    </select>
</div>
